@{
    ViewData["Title"] = "Add";
}
<div class="text-center">
    <h2 class="display-4">Добавление склада</h2>
</div>
<form method="post">
    <div class="row">
        <div class="col-4">Название:</div>
        <div class="col-8"><input type="text" name="name" id="name" /></div>
    </div>
    <div class="row">
        <div class="col-4">Ответственный:</div>
        <div class="col-8"><input type="text" name="responsibleName" id="responsibleName" /></div>
    </div>
    <div class="row">
        <div class="col-4"></div>
        <div class="col-8">
            <input type="submit" value="Добавить" class="btn btn-primary" />
        </div>
    </div>
</form>
